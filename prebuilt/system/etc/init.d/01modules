#!/system/bin/sh

# for mobile network (3G, HSDPA)
if [ ! -f /system/ppp ]; then
  insmod /system/lib/modules/msm_rmnet.ko
fi

# for VPN
if [ -f /system/lib/modules/tun.ko ]; then
  insmod /system/lib/modules/tun.ko
fi

